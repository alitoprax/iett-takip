{
    "version": 2,
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.py"
        },
        {
            "src": "/(index\\.html|mobile\\.js|mobile\\.css|manifest\\.json|sw\\.js)",
            "dest": "/public/$1"
        },
        {
            "src": "/mobile",
            "dest": "/public/index.html"
        },
        {
            "src": "/mobil",
            "dest": "/public/index.html"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}